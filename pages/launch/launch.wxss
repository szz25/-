.dai{
  width: 100%;
  background: #eee;
  padding: 17rpx;
  display: flex;
  font-size: 30rpx;
  justify-content: space-between;
  align-items: center;
  margin-top: 40rpx;
  box-sizing: border-box;
  border-radius: 3px;
}
.inp{
  width: 200rpx;
}
.sel{
  width:100%;
  height:100%;
  padding: 0 5%;
  box-sizing: border-box;
  border-top: 1px solid #eee;
}
.change{
  font-size: 26rpx;
  margin-top: 20rpx;
}
.change text{
  color: blue;
  display: inline;
  border: none;
  outline: none;
}
.foist{
  width: 100%;
  padding: 20rpx;
  background: blue;
  box-sizing: border-box;
  margin-top: 200rpx;
  text-align: center;
  color: #fff;
  border-radius: 10rpx;
}
.money{
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: flex-end;
  margin-top: 130rpx;
}
.h1{
  font-size: 60rpx;
  font-weight: bolder;
}