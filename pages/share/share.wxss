.foot{
  width: 100%;
  padding: 20rpx 50rpx;
  box-sizing: border-box;
  background: #ccc;
  text-align: center;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}

.pol{
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 20rpx;
}
.pol view{
  display: flex;
  flex-direction: column;
  text-align: center;
}
.pol image{
  width:100rpx;
  height: 100rpx;
  margin: 0 auto;
}
.share{
  width: 80%;
  height: 70%;
  background: #fff;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  border-radius: 10%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.sharec{
  width:200rpx;
  height: 200rpx;
  background: #ccc;
  text-align: center;
  line-height: 200rpx;
  border-radius: 50%;
  border: 2rpx solid #000;
}
.shareClick{
  width:200rpx;
  height: 200rpx;
  background: #ccc;
  text-align: center;
  line-height: 200rpx;
  border-radius: 50%;
  border: 2rpx solid #000;
  margin-top: 200rpx;
}
.h1{
  margin-top: 80rpx;
  font-weight: bold;
}
.h2{
  margin-top: 40rpx;
  font-size: 35rpx;
}

