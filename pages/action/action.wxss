.sle{
  width: 100%;
  height: 100%;
  text-align: center;
  padding: 40rpx;
  box-sizing: border-box;
  font-size: 0;
}
.h1{
  width: 70%;
  font-weight: bolder;
  font-size: 66rpx;
  margin: 0 auto;
  margin-top: 80rpx;
}
.h2{
  font-size: 40rpx;
}
.imgs{
  width: 50%;
  height: 340rpx;
  margin: 0 auto;
  margin-top: 50rpx;
}
.img{
  width: 100%;
  height: 100%;
}
.btn{
  margin-top: 50rpx;
  width: 60%;
  height:120rpx;
  line-height: 120rpx;
  background: blue;
  color: #fff;
  border-radius: 120rpx;
}
.child{
  width: 100%;
  border: 2rpx solid #ccc;
  height: 90%;
  border-radius: 50rpx;
}

/*对话框 */
.commodity_attr_box {
 height: 374rpx;
 width: 100%;
 overflow: hidden;
 position: absolute;
 bottom: 0;
 left: 0;
 z-index: 2000;
 background: #ccc;
 padding: 20rpx;
 text-align: center;
 box-sizing: border-box;
 font-size: 40rpx;
}
.fen{
  border: none;
  display: inline;
  background: #ccc;
  border: none;
}
.fen::after {
  border: none;
}

.pol{
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 20rpx;
}
.pol view{
  display: flex;
  flex-direction: column;
  text-align: center;
}
.pol image{
  width:100rpx;
  height: 100rpx;
  margin: 0 auto;
}